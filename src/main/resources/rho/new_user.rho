////////////////////
// add user
////////////////////
new userId, countOut , stdout(`rho:io:stderr`)in {
  @["Immersion", "newUserId"]!(*userId) |
  for(@newUser <- userId) {
      stdout!(["newUser",newUser ])
      | @["Immersion","playCount"]!(newUser, *countOut)
     | for(@count <- countOut) {
           stdout!(["count",count ])
     }
   }
}
