////////////////////
// add user
////////////////////
new userid, countout, testLog(`rho:io:stderr`) in {
  @["Immersion", "newUserId"]!(*userid)
  | for(@newuser <- userid) {
       testLog!(["newuser",newuser])
  | @["Immersion","playCount"]!(newuser,*countout)
 } | for(@cnt <- countout) {
    testLog!(["cnt",cnt])
}
}
